//printf count using recursion
#include <iostream>

using namespace std;

void PrintCount(int no)
{
    if(no==0)
    {
        return ;
    }
      cout<<no<<endl;
      PrintCount(no-1);
}
int main()
{
   int no;
   printf("Enter the number\n");
   scanf("%d",&no);
   
   
  PrintCount(no);
   

    return 0;
}
